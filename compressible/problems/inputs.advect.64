[driver]
max_steps = 500
tmax = 1.0

init_tstep_factor = 1.0
fix_dt = 0.005


[compressible]
limiter = 0
cvisc = 0.1


[io]
basename = advect_


[eos]
gamma = 1.4


[mesh]
nx = 64
ny = 64
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[vis]
dovis = 1
