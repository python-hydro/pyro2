# simple inputs files for the unsplit CTU hydro scheme

[driver]
max_steps = 500
tmax = 1.0

max_dt_change = 1.e33
init_tstep_factor = 1.0

cfl = 0.8


[io]
basename = test_
n_out = 10

[mesh]
nx = 64
ny = 64
xmax = 1.0
ymax = 1.0

xlboundary = outflow
xrboundary = outflow

ylboundary = outflow
yrboundary = outflow


[advection]
limiter = 2

[particles]
do_particles = 1
particle_generator = grid
