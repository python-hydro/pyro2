# simple inputs files for the incompressible convergence test

[driver]
max_steps = 1000
tmax = 0.1
cfl = 0.5
init_tstep_factor = 1.0
fix_dt = 1.25e-3


[incompressible_viscous]
viscosity = 0.1


[io]
basename = converge_viscous_128_
dt_out = 0.1


[mesh]
nx = 128
ny = 128
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[converge]



